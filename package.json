{
  "name": "nuxt-todo",
  "version": "1.0.0",
  "description": "A simple todo app with Nuxt.js",
  "private": true,
  "engines": {
    "node": "16.x",
    "npm": "8.19.x",
    "yarn": "1.22.x"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/i18n": "^7.3.1",
    "core-js": "^3.25.3",
    "date-fns": "^2.29.3",
    "nuxt": "^2.15.8",
    "nuxt-supabase": "1.0.8",
    "toastify-js": "^1.12.0",
    "uuid4": "^2.0.3",
    "vue": "^2.7.10",
    "vue-server-renderer": "^2.7.10",
    "vue-template-compiler": "^2.7.10"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@nuxtjs/eslint-config": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@vue/test-utils": "1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.3.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.5.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.7.1",
    "sass": "^1.57.1",
    "sass-loader": "10",
    "vue-jest": "^3.0.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sai-wtag/nuxt-todo"
  },
  "author": "Md Asiful Islam Sakib <asiful.sakib@welldev.io>",
  "license": "MIT"
}
